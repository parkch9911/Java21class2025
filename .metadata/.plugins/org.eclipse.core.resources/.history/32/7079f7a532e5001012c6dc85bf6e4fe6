package ifexample;

import java.io.IOException;

public class Readin {

	public static void main(String[] args) throws IOException {
		// System.in.read() => 키보드로 부터 값을 입력 받을 수 있다.
		int keycode;
		System.out.print("값을 입력하세요 : ");
		keycode = System.in.read();
		System.out.println("keycode : "+keycode);
	}

}
